<application>
  <component name="LafManager" autodetect="false">
    <laf class-name="com.intellij.ide.ui.laf.IntelliJLaf" themeId="JetBrainsLightTheme" />
    <preferred-light-laf class-name="com.intellij.ide.ui.laf.IntelliJLaf" themeId="JetBrainsLightTheme" />
    <preferred-dark-laf class-name="com.intellij.ide.ui.laf.darcula.DarculaLaf" />
    <lafs-to-previous-schemes>
      <laf-to-scheme laf="IntelliJ Light" scheme="_@user_IntelliJ Light" />
      <laf-to-scheme laf="Light" scheme="_@user_Light" />
    </lafs-to-previous-schemes>
  </component>
</application>